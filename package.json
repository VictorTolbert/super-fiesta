{
  "name": "@victortolbert/titan-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:docs": "vuepress dev docs",
    "serve:styleguide": "vue-cli-service styleguidist",
    "build": "rimraf ../public/{js,css,img} && vue-cli-service build --no-clean",
    "build:docs": "vuepress build docs",
    "build:styleguide": "vue-cli-service styleguidist:build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "clean:symlinks": "rm -rf src/assets/css/shared src/assets/svg/shared src/assets/font/shared src/components/shared src/filters/shared src/utilities/shared src/services/shared",
    "test:unit:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --no-cache --runInBand"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "buefy": "^0.8.5",
    "clipboard": "^2.0.4",
    "core-js": "^3.3.2",
    "dog-names": "^1.0.2",
    "firebase": "^7.2.1",
    "gsap": "^2.0.2",
    "jquery": "3.4.1",
    "mini-css-extract-plugin": "^0.5.0",
    "nprogress": "0.2.0",
    "portal-vue": "^2.1.6",
    "register-service-worker": "^1.6.2",
    "tiptap": "^1.26.4",
    "tiptap-extensions": "^1.28.4",
    "v-click-outside": "^2.1.4",
    "v-money": "^0.8.1",
    "v-tooltip": "^2.0.2",
    "vue": "^2.6.10",
    "vue-analytics": "^5.17.2",
    "vue-carousel": "^0.18.0",
    "vue-clickaway": "^2.2.2",
    "vue-clipboard2": "^0.3.1",
    "vue-confetti": "^2.0.5",
    "vue-croppa": "^1.3.8",
    "vue-dummy": "^1.1.3",
    "vue-i18n": "^8.15.0",
    "vue-meta": "^2.3.1",
    "vue-multiselect": "^2.1.6",
    "vue-router": "^3.1.3",
    "vue-select": "^3.2.0",
    "vue-slick": "^1.1.15",
    "vue-template-compiler": "^2.6.10",
    "vue-the-mask": "^0.11.1",
    "vuejs-datepicker": "1.6.2",
    "vuelidate": "0.7.4",
    "vuex": "^3.1.1",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@fortawesome/fontawesome-pro": "^5.11.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-regular-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/pro-light-svg-icons": "^5.11.2",
    "@fortawesome/pro-regular-svg-icons": "^5.11.2",
    "@fortawesome/pro-solid-svg-icons": "^5.11.2",
    "@fortawesome/vue-fontawesome": "^0.1.4",
    "@kazupon/vue-i18n-loader": "^0.4.1",
    "@tailwindcss/custom-forms": "^0.2.0",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-e2e-cypress": "^4.0.0",
    "@vue/cli-plugin-eslint": "4.0.1",
    "@vue/cli-plugin-pwa": "^4.0.0",
    "@vue/cli-plugin-unit-jest": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "flush-promises": "^1.0.0",
    "json-server": "^0.15.1",
    "lint-staged": "^9.4.2",
    "lodash.clonedeep": "^4.5.0",
    "node-sass": "^4.12.0",
    "postcss-calc": "^7",
    "postcss-cli": "^6.1.3",
    "postcss-nested": "^4.1.2",
    "rimraf": "^3.0.0",
    "sass": "^1.23.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^0.19.0",
    "style-resources-loader": "^1.2.1",
    "tailwindcss": "^1.1.2",
    "tailwindcss-aspect-ratio": "^1.0.3",
    "tailwindcss-elevation": "^0.3.1",
    "tailwindcss-responsive-embed": "^0.2.2",
    "tailwindcss-scrims": "^1.0.0",
    "theo": "^8.1.1",
    "vue-cli-plugin-i18n": "^0.6.0",
    "vue-cli-plugin-pug": "^1.0.7",
    "vue-cli-plugin-style-resources-loader": "0.1.3",
    "vue-cli-plugin-styleguidist": "3.25.0",
    "vue-loader": "^15.2.4",
    "vuepress": "^1.2.0",
    "webpack": "^4.41.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
